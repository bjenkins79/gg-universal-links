{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/.well-known/apple-app-site-association",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/invite/(.*)",
      "destination": "/reset-fallback.html",
      "statusCode": 200
    },
    {
      "source": "/reset/(.*)", 
      "destination": "/reset-fallback.html",
      "statusCode": 200
    }
  ]
}
